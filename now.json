{
  "env": {
    "ADOBE_API_KEY": "@adobe-api-key",
    "ADOBE_CLIENT_SECRET": "@adobe-client-secret",
    "NOW_API_TOKEN": "@now-api-token"
  },
  "routes": [
    {
      "src": "/img/(.*)",
      "headers": { "Cache-Control": "s-maxage=604800" },
      "dest": "/img/$1"
    },
    {
      "src": "/*.js",
      "headers": { "Cache-Control": "s-maxage=604800" }
    },
    {
      "src": "/*.css",
      "headers": { "Cache-Control": "s-maxage=604800" }
    }
  ]
}
